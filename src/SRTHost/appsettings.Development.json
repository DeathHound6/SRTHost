{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "DevelopmentHttp": {
        "Url": "http://*:7191",
        "Protocols": "Http1"
      },
      "DevelopmentHttps": {
        "Url": "https://*:7192",
        "Protocols": "Http1AndHttp2AndHttp3",
        "Certificate": {
          "Subject": "localhost",
          "Store": "My",
          "Location": "CurrentUser",
          "AllowInvalid": true
        },
        "ClientCertificateMode": "NoCertificate"
      }
    }
  }
}
